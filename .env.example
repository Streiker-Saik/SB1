# Создайте файл .env из копии этого файла и замените значения переменных реальными данными

# Ключ
SECRET_KEY=your_django_key_here

# Режим отладки
DEBUG=True # True - для разработки, False - для продакшена

# Настройки базы данных
POSTGRES_DB=your_database_name_here
POSTGRES_USER=your_user_name_here
POSTGRES_PASSWORD=your_password_here
POSTGRES_HOST=localhost   # Хост базы данных (обычно localhost)
POSTGRES_PORT=5432        # Порт, на котором работает база данных (стандартный порт для PostgreSQL)